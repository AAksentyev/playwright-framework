{
  "name": "playwright_framework",
  "version": "1.0.0",
  "description": "A portable plug-and-play Playwright framework. Includes a library of decorators, API testing support, reporting, and more.",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "test:headed": "npx playwright test --headed",
    "test:debug": "npx playwright test --debug",
    "test:api": "npx playwright test --grep \"@api\"",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AAksentyev/playwright_framework.git"
  },
  "keywords": [],
  "author": "Alex Aksentyev",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/AAksentyev/playwright_framework/issues"
  },
  "homepage": "https://github.com/AAksentyev/playwright_framework#readme",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.56.1",
    "@types/ajv": "^0.0.5",
    "@types/node": "^24.10.0",
    "@types/sprintf-js": "^1.1.4",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-playwright": "^2.3.0",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "reflect-metadata": "^0.2.2",
    "sprintf-js": "^1.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
